DCLAR SUBROUTIN main{StRiNg() argv______} <- InTgR [
     DCLAR CONSTANT StRiNg valu______ = CALL STDIN,givm{}/
     CHCK IF { valu______ IS *0* } DO [
		CALL printto{ STDOUT. *0* }/
     ] LS DO [
		CHCK UNTIL { NO } DO [
			CALL printto{ STDOUT. *1* }/
		]
     ]
]