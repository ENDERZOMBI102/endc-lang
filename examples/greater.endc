DCLAR SUBROUTIN main{StRiNg() argv______} <- InTgR [
     |* aka 23 >= 22 -> false
     *|
     CHCK IF { 23 =< 22 } DO [
          DCLAR CONSTANT StRiNg falsytxt__ = *a greater/equal to b*/
          CALL printto{ STDOUT. falsytxt__ }/
     ] LS DO [
          DCLAR CONSTANT StRiNg truthytxt__ = *a smoller than b*/
